// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model reviews {
  id String @id
  title String
  author String
  price Int
  publisher String
  published String
  image String
  read DateTime @default(now())
  memo String
}
